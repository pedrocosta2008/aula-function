let numero1 = Number(prompt("digite um numero"))
let numero2 = Number(prompt("digite outro numero"))

    function somadenumeros(numero1,numero2){
return numero1 + numero2
    }

function subtracaodenumeros(numero1,numero2){
return numero1 - numero2
  }

 function multiplicaçãodenumeros(numero1,numero2){
  return numero1 * numero2
    }

 function divisaodenumeros(numero1,numero2){
 return numero1/ numero2               
   }

   console.log(somadenumeros(numero1,numero2))
